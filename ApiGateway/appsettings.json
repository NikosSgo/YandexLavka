{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:5173",
      "http://localhost"
    ]
  },
  "ReverseProxy": {
    "Routes": {
      "users-service-users": {
        "ClusterId": "users-service",
        "Match": {
          "Path": "/api/users/{**catchall}"
        }
      },
      "users-service-addresses": {
        "ClusterId": "users-service",
        "Match": {
          "Path": "/api/addresses/{**catchall}"
        }
      },
      "warehouse-service-orders": {
        "ClusterId": "warehouse-service",
        "Match": {
          "Path": "/api/orders/{**catchall}"
        }
      },
      "warehouse-service-picking": {
        "ClusterId": "warehouse-service",
        "Match": {
          "Path": "/api/picking/{**catchall}"
        }
      },
      "warehouse-service-stock": {
        "ClusterId": "warehouse-service",
        "Match": {
          "Path": "/api/stock/{**catchall}"
        }
      },
      "warehouse-service-health": {
        "ClusterId": "warehouse-service",
        "Match": {
          "Path": "/api/health/{**catchall}"
        }
      },
      "users-service-swagger-json": {
        "ClusterId": "users-service",
        "Match": {
          "Path": "/swagger/users/v1/swagger.json"
        },
        "Transforms": [
          {
            "PathPattern": "/swagger/v1/swagger.json"
          }
        ]
      },
      "warehouse-service-swagger-json": {
        "ClusterId": "warehouse-service",
        "Match": {
          "Path": "/swagger/warehouse/v1/swagger.json"
        },
        "Transforms": [
          {
            "PathPattern": "/swagger/v1/swagger.json"
          }
        ]
      },
      "auth-service-auth": {
        "ClusterId": "auth-service",
        "Match": {
          "Path": "/api/auth/{**catchall}"
        }
      },
      "auth-service-swagger-json": {
        "ClusterId": "auth-service",
        "Match": {
          "Path": "/swagger/auth/v1/swagger.json"
        },
        "Transforms": [
          {
            "PathPattern": "/swagger/v1/swagger.json"
          }
        ]
      }
    },
    "Clusters": {
      "users-service": {
        "Destinations": {
          "primary": {
            "Address": "http://userservice-api:8080/"
          }
        }
      },
      "warehouse-service": {
        "Destinations": {
          "primary": {
            "Address": "http://warehouse-service-api:8080/"
          }
        }
      },
      "auth-service": {
        "Destinations": {
          "primary": {
            "Address": "http://authservice-api:8080/"
          }
        }
      }
    }
  }
}
